{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Financial Datasets",
  "description": "Financial Datasets is a stock market API with financial data for 30,000+ tickers over 30+ years, including financial statements, equity prices, insider transactions, SEC filings, and more.",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg",
    "href": "https://financialdatasets.ai"
  },
  "favicon": "/favicon.svg",
  "seo": {
    "metatags": {
      "image": "/images/new-docs.png",
      "type": "website",
      "title": "Financial Datasets - API Documentation",
      "description": "Financial Datasets is a stock market API with financial data for 30,000+ tickers over 30+ years, including financial statements, equity prices, insider transactions, SEC filings, and more."
    }
  },
  "colors": {
    "primary": "#26251E",
    "light": "#F7F7F4",
    "dark": "#14120B"
  },
  "background": {
    "color": {
      "light": "#F7F7F4",
      "dark": "#14120B"
    }
  },
  "navbar": {
